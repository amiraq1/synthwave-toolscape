    "tools:restore:dry": "node scripts/restore-tools-from-backup.js --backup-dir=backups",
    "tools:restore:apply": "node scripts/restore-tools-from-backup.js --backup-dir=backups --apply",
    "tools:dead-links:dry": "node scripts/delete-dead-links.js --dry-run --timeout-ms=12000 --concurrency=24 --retries=1 --backup-dir=backups",
    "tools:dead-links:apply": "node scripts/delete-dead-links.js --apply --timeout-ms=12000 --concurrency=24 --retries=1 --backup-dir=backups",
